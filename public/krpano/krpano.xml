	               maps_type="google"
	               maps_bing_api_key=""
	               maps_google_api_key=""
	               maps_zoombuttons="false"
	               gyro="false"
	               webvr="false"
	               webvr_gyro_keeplookingdirection="false"
	               webvr_prev_next_hotspots="false"
	               littleplanetintro="false"
	               title="true"
	               thumbs="true"
